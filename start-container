#!/usr/bin/env bash
if [ ! -z "$WWWUSER" ]; then usermod -u $WWWUSER sail fi
if [ ! -d /.composer ]; then mkdir /.composer fi
chmod -R ugo+rw /.composer
if test -f "setup"; then
    echo "Running setup"
    chmod 755 setup
    ./setup
else
    echo "Setup does not exists"
fi
# Start Supervisor
service supervisor start